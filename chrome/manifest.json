{
   "name": "Shaved Bieber",
   "description": "Clean up those lower regions of websites from Bieber mentions.",
   "website": "http://fffff.at/shaved-bieber",

   "background_page": "background.html",
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "jquery-latest.js" ],
      "matches": [ "http://*/*" ],
      "run_at": "document_start"
   } ],
   "icons": {
      "16": "icons/shaved-bieber16.png",
      "32": "icons/shaved-bieber32.png",
      "48": "icons/shaved-bieber48.png",
      "64": "icons/shaved-bieber64.png",
      "128": "icons/shaved-bieber128.png",
      "256": "icons/shaved-bieber256.png"
   },
   "permissions": [ "tabs", "http://*/*" ],
   "update_url": "http://addons.gleuch.com/shaved-bieber/updates/chrome?data=",
   "version": "1.0.0"
}
